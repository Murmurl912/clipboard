<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <VBox maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
      <padding>
         <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
      </padding>
      <Label text="New Password">
         <padding>
            <Insets bottom="5.0"/>
         </padding>
      </Label>
      <PasswordField fx:id="password"/>
      <Label layoutX="20.0" layoutY="20.0" text="New Password">
         <padding>
            <Insets bottom="5.0" top="5.0"/>
         </padding>
      </Label>
      <PasswordField layoutX="20.0" layoutY="41.0" fx:id="passwordConfirm"/>
      <Label text="Email Verification Code">
         <padding>
            <Insets bottom="5.0" top="5.0"/>
         </padding>
      </Label>
      <TextField layoutX="20.0" layoutY="41.0"/>
      <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS"/>
      <HBox>
         <VBox.margin>
            <Insets/>
         </VBox.margin>
         <JFXButton maxWidth="1.7976931348623157E308" text="Send" fx:id="send">
            <HBox.margin>
               <Insets right="20.0"/>
            </HBox.margin>
         </JFXButton>
         <Label maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS"/>
         <JFXButton layoutX="327.0" layoutY="10.0" maxWidth="1.7976931348623157E308" text="Cancel" fx:id="cancel"/>
         <JFXButton layoutX="10.0" layoutY="10.0" maxWidth="1.7976931348623157E308" text="Confirm" fx:id="confirm">
            <HBox.margin>
               <Insets left="20.0"/>
            </HBox.margin>
         </JFXButton>
      </HBox>
   </VBox>
</VBox>
